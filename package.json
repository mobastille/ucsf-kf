{
  "name": "ucsf-portal-ui",
  "version": "1.4.0",
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint&fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "start": "DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "build:netlify": "npm ci && npm run build",
    "test": "react-scripts test --coverage --coverageReporters=\"json-summary\"",
    "test:github": "react-scripts test --coverage --watchAll=false --no-watchman --coverageReporters=\"json-summary\"",
    "coverage": "npm test -- --watchAll=false --coverage --coverageReporters=\"json-summary\"",
    "coverage:changed": "npm test -- --coverage --watchAll=false --onlyChanged --coverageReporters=\"json-summary\"",
    "lint": "eslint $(git diff-index --name-only --diff-filter=d HEAD | grep \"[\\.js|\\.jsx|\\.tsx|\\.ts]$\" | xargs)",
    "lint:fix": "eslint --fix --rule 'no-console: 2'",
    "theme": "mkdir -p src/style/themes/ucsf/dist && npm run theme:antd",
    "theme:watch": "nodemon --watch './src/style/themes' -e scss,less --exec 'npm run theme'",
    "theme:antd": "npm run theme:antd:colors && npm run theme:antd:main",
    "theme:antd:main": "lessc --js src/style/themes/ucsf/antd-ucsf-theme.less src/style/themes/ucsf/dist/antd.css",
    "theme:antd:colors": "less2sass ./src/style/themes/ucsf/colors.less && mv -f ./src/style/themes/ucsf/colors.scss ./src/style/themes/ucsf/dist/_colors.scss",
    "eject": "react-scripts eject",
    "prepare": "npm run theme",
    "storybook": "DISABLE_ESLINT_PLUGIN=true start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ucsf/kf-portal-ui.git"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@apollo/client": "^3.5.10",
    "@dnd-kit/core": "^4.0.3",
    "@dnd-kit/sortable": "^5.1.0",
    "@ferlab/ui": "^5.5.0",
    "@loadable/component": "^5.15.2",
    "@nivo/bar": "^0.79.1",
    "@nivo/core": "^0.80.0",
    "@nivo/pie": "^0.79.1",
    "@react-keycloak/core": "^3.2.0",
    "@react-keycloak/web": "^3.4.0",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/d3": "^7.1.0",
    "@types/file-saver": "^2.0.2",
    "@types/loadable__component": "^5.13.4",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.2",
    "antd": "^4.20.0",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "d3": "^5.16.0",
    "date-fns": "2.29.3",
    "file-saver": "^2.0.2",
    "graphql": "^16.2.0",
    "html-react-parser": "^1.4.4",
    "jwt-decode": "^3.1.2",
    "keycloak-js": "^15.0.2",
    "lodash": "^4.17.21",
    "rc-menu": "^9.6.0",
    "rc-tree-select": "^5.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.18.0",
    "react-intl-universal": "^2.4.8",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.20",
    "sass": "^1.53.0",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@ferlab/eslint-config": "^1.0.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/node-logger": "^6.5.13",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "less": "^4.1.3",
    "less2sass": "^1.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.0.5",
    "prettier-eslint": "^10.1.1",
    "prop-types": "^15.8.1",
    "webpack": "^5.75.0"
  }
}
